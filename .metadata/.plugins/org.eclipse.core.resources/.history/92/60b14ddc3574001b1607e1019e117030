package practice;

import java.util.ArrayList;

public class ArrayListExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		ArrayList<String> a = new ArrayList<String>();
		a.add("Nikhil");
		a.add("Raja");
		a.add("Niharika");
		System.out.println(a);
		a.add(0, "Selenium");
		System.out.println(a);
		a.remove(2);
		System.out.println(a);
		
	}

}
